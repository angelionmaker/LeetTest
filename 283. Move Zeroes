public class Solution {
    public void moveZeroes(int[] nums) {
        int n = nums.length;
        int pos = 0;
        for (int i =0; i< n; i++){
            if(nums[i]!=0){
                nums[pos] = nums[i];
                pos++;
            }
        }
        for(;pos<n;pos++){
            nums[pos]=0;
        }
        
    }
}

